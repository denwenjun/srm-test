pipeline {
  agent any
  stages {
    stage('checkout') {
      steps {
        sh 'pwd'
      }
    }

    stage('build') {
      steps {
        sh 'ls'
      }
    }

    stage('docker build ') {
      steps {
        sh 'ls'
      }
    }

    stage('docker push') {
      steps {
        sh 'ls'
      }
    }

    stage('k8s deploy') {
      steps {
        sh 'ls'
      }
    }

  }
}